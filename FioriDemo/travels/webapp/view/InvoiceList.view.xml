<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <List
        id="list-invoices"
        headerText="{i18n>invoiceListTitle}"
        class="sapUiResponsiveMargin"
        width="auto"
        items="{invoice>/Invoices}"
    >
        <items>
            <ObjectListItem
                id="item-invoice"
                title="{invoice>Quantity} x {invoice>ProductName}"
            />
        </items>
    </List>
</mvc:View>
