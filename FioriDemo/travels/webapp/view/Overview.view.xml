<mvc:View
    controllerName="ns.travels.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
>
    <Page
        id="page-overview"
        title="{i18n>homePageTitle}"
    >
        <content>
            <mvc:XMLView
                id="hello-view"
                viewName="ns.travels.view.HelloPanel"
            />
            <mvc:XMLView
                id="invoice-list-view"
                viewName="ns.travels.view.InvoiceList"
            />
        </content>
    </Page>
</mvc:View>
